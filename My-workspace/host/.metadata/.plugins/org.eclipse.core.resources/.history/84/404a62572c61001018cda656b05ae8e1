#include <stdio.h>
#include <stdint.h>

uint8_t age;
uint32_t num1, num2;

int main (void){

	printf("Enter you two numbers");

	scanf("%d %d",&num1, &num2);

	if (num1==num2){
		printf("Numbers are equal\n");
	}else{
		if(num1<num2)
			printf("%d is greater number\n",num2);
		else
			printf("%d is greater number\n",num1);
	}


	for (;;);
}
